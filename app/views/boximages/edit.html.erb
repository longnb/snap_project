<% provide(:title, "Edit Boximage") %> 
<h1>Update Boximage</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@boximage) do |f| %>
      <%= render 'shared/error_messages_boximage' %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_area :description %>
      <%= f.hidden_field :user_id, :value => current_user.id %>
       <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
        <%#= gravatar_for2 @project %> 
  </div>
</div>
